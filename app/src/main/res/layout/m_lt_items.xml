<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    android:elevation="8dp"
    android:paddingBottom="12dp"
    android:orientation="vertical"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView11">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorBorderCL" />



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/photoVideo_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/cl_bg"
        android:padding="8dp">

        <TextView
            android:id="@+id/tv_t_link"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="Photo and Video Transfer"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tv_d_link"
            app:layout_constraintStart_toEndOf="@+id/iv_t_link"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/tv_d_link"
            android:textColor="@color/colorTextTwo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="No data to share/backup files/SMS/Contact"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tv_t_link"
            app:layout_constraintTop_toBottomOf="@+id/tv_t_link" />

        <ImageView
            android:id="@+id/iv_t_link"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_photo_and_video_transfer" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/smartCharge_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/cl_bg"
        android:padding="8dp">

        <TextView
            android:id="@+id/smartCharge_tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="Smart Charge"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/smartCharge_tv2"
            app:layout_constraintStart_toEndOf="@+id/smartCharge_iv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/smartCharge_tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Remind you charging state to avoid overcharging"
            android:textColor="@color/colorTextTwo"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/smartCharge_tv1"
            app:layout_constraintTop_toBottomOf="@+id/smartCharge_tv1" />

        <ImageView
            android:id="@+id/smartCharge_iv"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_smart_charge" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cleanWhatsApp_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/cl_bg"
        android:padding="8dp">

        <TextView
            android:id="@+id/cleanWhatsApp_tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="Clean WhatsApp"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/cleanWhatsApp_tv2"
            app:layout_constraintStart_toEndOf="@+id/cleanWhatsApp_iv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:textColor="@color/colorTextTwo"
            android:id="@+id/cleanWhatsApp_tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Delete useless file in WhatsApp"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/cleanWhatsApp_tv1"
            app:layout_constraintTop_toBottomOf="@+id/cleanWhatsApp_tv1" />

        <ImageView
            android:id="@+id/cleanWhatsApp_iv"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_clean_whatsapp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/deepClean_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/cl_bg"
        android:padding="8dp">

        <TextView
            android:id="@+id/visitToAppStore_tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="Deep Clean"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/visitToAppStore_tv2"
            app:layout_constraintStart_toEndOf="@+id/visitToAppStore_iv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/visitToAppStore_tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Clean up files deeply to release nore space"
            android:textColor="@color/colorTextTwo"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/visitToAppStore_tv1"
            app:layout_constraintTop_toBottomOf="@+id/visitToAppStore_tv1" />

        <ImageView
            android:id="@+id/visitToAppStore_iv"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_smart_cleanup" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/speedTest_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/cl_bg"
        android:padding="8dp">

        <TextView
            android:id="@+id/tv_s_internet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="Internet Speed"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tv_d_internet"
            app:layout_constraintStart_toEndOf="@+id/iv_s_internet"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/tv_d_internet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="You can test your internet speed "
            android:textColor="@color/colorTextTwo"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tv_s_internet"
            app:layout_constraintTop_toBottomOf="@+id/tv_s_internet" />

        <ImageView
            android:id="@+id/iv_s_internet"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_speed_test" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</LinearLayout>